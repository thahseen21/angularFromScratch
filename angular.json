{
  "projects": {
    "AngularFromScratch": {
      "projectType": "application",
      "root": "",
      "prefix": "app",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.json"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angularfromscratch:build:production"
            },
            "development": {
              "browserTarget": "angularfromscratch:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
